double * metricSpaceReader(char * filename, int n, int d){

    double val1,val2;
    int cnt,i=0;
    FILE *fp = fopen(filename,"r");
    double *array=malloc(d*n*sizeof(double));

    fscanf(f[, "%lf %lf",&val1,&val2);

    for(int row=0; row<n; row++){
        cnt=0;
        while(cnt++<d)
            fscanf(fp, "%lf ",&array[i++]);
        cnt=0
    }
    return array;
}

}
